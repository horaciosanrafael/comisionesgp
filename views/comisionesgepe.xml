<odoo>
 <record id="id_cliente_vendedor" model="ir.ui.view">
        <field name="name">res.partner.cliente.comercial</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[last()]" position="inside">
                <page string="Comisiones GEPE" name="gestion_de_comisiones">
                    <field name = "comisiones_id"/>
                </page>
            </xpath>
        </field>
    </record>
</odoo>